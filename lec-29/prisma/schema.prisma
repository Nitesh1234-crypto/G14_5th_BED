generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement()) //@id---->primary key
  email   String   @unique //unique index-->cannot be repeated
  name    String? @default("xyz")
}
//1st insert --->1 user-1
//2nd insert --->2 user-2
//3rd insert --->3 x
//4th insert----->4 user--4
